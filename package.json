{
  "name": "my-qwik-basic-starter",
  "description": "App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "npm run build.client && npm run build.cloudflare && npm run build.netlify",
    "build.client": "vite build",
    "build.cloudflare": "vite build -c adaptors/cloudflare-pages/vite.config.ts",
    "build.netlify": "vite build -c adaptors/netlify-edge/vite.config.ts",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "netlify.deploy": "netlify deploy",
    "netlify.dev": "netlify dev",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.13.0-dev20221109155930",
    "@builder.io/qwik-city": "0.0.120",
    "@types/eslint": "8.4.9",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "netlify-cli": "^12.0.11",
    "node-fetch": "3.2.10",
    "prettier": "2.7.1",
    "typescript": "4.8.4",
    "vite": "3.2.2",
    "vite-tsconfig-paths": "3.5.0",
    "wrangler": "latest"
  }
}
