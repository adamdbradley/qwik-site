{
  "name": "my-qwik-basic-starter",
  "description": "App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build.client": "vite build",
    "build.cloudflare": "npm run build.client && vite build -c adaptors/cloudflare-pages/vite.config.ts",
    "build.express": "vite build -c adaptors/express/vite.config.ts",
    "build.netlify": "npm run build.client && vite build -c adaptors/netlify-edge/vite.config.ts",
    "build.vercel": "npm run build.client && vite build -c adaptors/vercel-edge/vite.config.ts",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "netlify.deploy": "netlify deploy",
    "netlify.dev": "netlify dev",
    "preview": "qwik build preview && vite preview --open",
    "serve.cloudflare": "wrangler pages dev ./dist",
    "serve.express": "node .express/entry.express.js",
    "serve.netlify": "netlify deploy",
    "serve.vercel": "vercel deploy",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.16.1",
    "@builder.io/qwik-city": "file:./fake_modules/lib",
    "@types/compression": "^1.7.2",
    "@types/eslint": "8.4.9",
    "@types/express": "^4.17.14",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "netlify-cli": "^12.0.11",
    "node-fetch": "3.2.10",
    "prettier": "2.7.1",
    "typescript": "4.8.4",
    "vercel": "^28.4.17",
    "vite": "3.2.2",
    "vite-tsconfig-paths": "3.5.0",
    "wrangler": "latest"
  }
}
